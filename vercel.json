{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run start:dev",
  "builds": [
    {
        "src": "src/main.ts",
        "use": "@vercel/node"
    }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "src/main.ts",
          "methods": [ "GET", "POST", "PUT", "PATCH", "DELETE" ]
      }
  ]
}